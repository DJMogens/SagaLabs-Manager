<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="1080.0" prefWidth="1920.0" stylesheets="@Stylesheet.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sagalabsmanagerclient.controllers.MachinesController">
   <center>
       <TabPane fx:id="tabPane" prefHeight="1080.0" prefWidth="1100.0" stylesheets="@Stylesheet.css" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
          <tabs>
              <Tab fx:id="allTab" text="All">
                  <content>
                      <TableView fx:id="allTableView" prefHeight="450.0" prefWidth="397.0">
                          <columns>
                              <TableColumn fx:id="selectColumn" prefWidth="50">
                                  <cellValueFactory>
                                      <PropertyValueFactory property="select" />
                                  </cellValueFactory>
                              </TableColumn>
                              <TableColumn fx:id="idColumn" prefWidth="20.0" text="ID">
                                  <cellValueFactory>
                                      <PropertyValueFactory property="id" />
                                  </cellValueFactory>
                              </TableColumn>
                              <TableColumn fx:id="vmColumn" prefWidth="250.0" text="Machine Name">
                                  <cellValueFactory>
                                      <PropertyValueFactory property="vmName" />
                                  </cellValueFactory>
                              </TableColumn>
                              <TableColumn fx:id="osColumn" prefWidth="100.0" text="OS">
                                  <cellValueFactory>
                                      <PropertyValueFactory property="os" />
                                  </cellValueFactory>
                              </TableColumn>
                              <TableColumn fx:id="stateColumn" prefWidth="99.0" text="State">
                                  <cellValueFactory>
                                      <PropertyValueFactory property="state" />
                                  </cellValueFactory>
                              </TableColumn>
                              <TableColumn fx:id="ipColumn" prefWidth="200.0" text="IP Address">
                                  <cellValueFactory>
                                      <PropertyValueFactory property="ip" />
                                  </cellValueFactory>
                              </TableColumn>
                              <TableColumn fx:id="rgColumn" prefWidth="200.0" text="Resource Group">
                                  <cellValueFactory>
                                      <PropertyValueFactory property="resourceGroup" />
                                  </cellValueFactory>
                              </TableColumn>
                          </columns>
                      </TableView>
                  </content>
              </Tab>
          </tabs>
       </TabPane>
   </center>
   <left>
      <VBox alignment="TOP_RIGHT" prefHeight="1080.0" prefWidth="280.0" styleClass="root2" BorderPane.alignment="CENTER">
         <children>
            <Region prefHeight="100.0" prefWidth="280.0" />
                <Button mnemonicParsing="false" onAction="#home" prefHeight="100.0" prefWidth="240.0" styleClass="button3" text="Home">
               <font>
                  <Font size="33.0" />
               </font>
            </Button>
            <Region prefHeight="50.0" prefWidth="280.0" />
                <Button mnemonicParsing="false" onAction="#switchToMachine" prefHeight="100.0" prefWidth="240.0" text="Machines">
               <font>
                  <Font size="33.0" />
               </font>
               <styleClass>
                  <String fx:value="button3" />
                  <String fx:value="button4" />
               </styleClass>
            </Button>
            <Region prefHeight="50.0" prefWidth="280.0" />
                <Button mnemonicParsing="false" onAction="#switchToSQL" prefHeight="100.0" prefWidth="240.0" styleClass="button3" text="SQL Connect">
               <font>
                  <Font size="33.0" />
               </font>
            </Button>
            <Region layoutX="10.0" layoutY="360.0" prefHeight="50.0" prefWidth="280.0" />
            <Button layoutX="50.0" layoutY="411.0" mnemonicParsing="false" onAction="#switchToVPN" prefHeight="100.0" prefWidth="240.0" styleClass="button3" text="VPN">
               <font>
                  <Font size="33.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </left>
   <right>
      <VBox alignment="TOP_CENTER" prefHeight="1080.0" prefWidth="540.0" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="TOP_RIGHT">
               <children>
                    <Button mnemonicParsing="false" onAction="#logout" prefHeight="73.0" prefWidth="174.0" styleClass="button1" text="Logout">
                     <font>
                        <Font size="30.0" />
                     </font>
                     <HBox.margin>
                        <Insets right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <Accordion>
              <panes>
                <TitledPane animated="false" text="Power Control">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="538.0">
                           <children>
                              <Button layoutX="61.0" layoutY="6.0" mnemonicParsing="false" prefHeight="88.0" prefWidth="133.0" text="Turn on" />
                              <Button layoutX="299.0" layoutY="6.0" mnemonicParsing="false" prefHeight="88.0" prefWidth="133.0" text="Turn Off" />
                           </children>
                        </AnchorPane>
                  </content>
                </TitledPane>
                <TitledPane animated="false" text="Filters">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="245.0" prefWidth="538.0">
                           <children>
                              <TextField fx:id="nameFilterText" layoutX="5.0" layoutY="14.0" prefHeight="20.0" prefWidth="369.0" promptText="Name">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </TextField>
                               <TextField fx:id="stateFilterText" layoutX="4.0" layoutY="60.0" prefHeight="46.0" prefWidth="371.0" promptText="State">
                                   <font>
                                       <Font size="20.0" />
                                   </font>
                               </TextField>
                              <TextField fx:id="osFilterText" layoutX="5.0" layoutY="106.0" prefHeight="20.0" prefWidth="369.0" promptText="OS">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </TextField>
                              <TextField fx:id="ipFilterText" layoutX="5.0" layoutY="152.0" prefHeight="20.0" prefWidth="369.0" promptText="IP">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </TextField>
                              <Button fx:id="filterButton" alignment="CENTER" contentDisplay="CENTER" layoutX="380.0" layoutY="61.0" mnemonicParsing="false" onAction="#applyFilter" prefHeight="44.0" prefWidth="159.0" text="Apply Filter">
                                 <styleClass>
                                    <String fx:value="button" />
                                    <String fx:value="button1" />
                                 </styleClass>
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </Button>
                              <Button fx:id="resetFilterButton" alignment="CENTER" contentDisplay="RIGHT" layoutX="381.0" layoutY="149.0" mnemonicParsing="false" onAction="#resetFilters" prefHeight="43.0" prefWidth="158.0" text="Reset filters">
                                 <styleClass>
                                    <String fx:value="button" />
                                    <String fx:value="button1" />
                                 </styleClass>
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </Button>
                           </children>
                        </AnchorPane>
                  </content>
                </TitledPane>
                <TitledPane animated="false" prefHeight="1146.0" prefWidth="540.0" text="Run Commands">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="330.0" prefWidth="538.0">
                           <children>
                              <TextArea prefHeight="480.0" prefWidth="534.0" promptText="Type command to run on selected machines:" style="-fx-background-color: 55;" styleClass="text-area" stylesheets="@Stylesheet.css">
                                 <font>
                                    <Font size="28.0" />
                                 </font>
                              </TextArea>
                              <Button layoutX="390.0" layoutY="489.0" mnemonicParsing="false" prefHeight="65.0" prefWidth="134.0" styleClass="button1" text="Run">
                                 <font>
                                    <Font size="30.0" />
                                 </font>
                              </Button>
                           </children>
                        </AnchorPane>
                  </content>
                </TitledPane>
              </panes>
            </Accordion>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="311.0">
               <children>
                  <Region prefHeight="105.0" prefWidth="540.0" />
                  <HBox alignment="TOP_RIGHT" prefHeight="50.0" prefWidth="540.0">
                     <children>
                        <Region prefHeight="76.0" prefWidth="56.0" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <Region prefHeight="30.0" prefWidth="540.0" />
         </children>
      </VBox>
   </right>
</BorderPane>
