<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="1080.0" prefWidth="1920.0" stylesheets="@Stylesheet.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sagalabsmanager.controllers.LabsController">
   <center>
        <TabPane prefHeight="1080.0" prefWidth="1100.0" stylesheets="@Stylesheet.css" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
            <tabs>
                <Tab text="All">
                    <content>
                          <TableView prefHeight="451.0" prefWidth="397.0">
                              <columns>
                                  <TableColumn prefWidth="450.0" text="ID" />
                                  <TableColumn minWidth="0.0" prefWidth="450.0" text="Machine Name" />
                                  <TableColumn prefWidth="199.0" text="OS" />
                              </columns>
                          </TableView>
                    </content>
                </Tab>
                <Tab text="Lab 1">
                    <content>
                          <TableView fx:id="lab1" prefHeight="452.0" prefWidth="397.0">
                              <columns>
                                  <TableColumn prefWidth="450.0" text="ID" />
                                  <TableColumn fx:id="lab1name" prefWidth="450.0" text="Machine Name" />
                                  <TableColumn fx:id="lab1os" prefWidth="199.0" text="OS" />
                              </columns>
                          </TableView>
                    </content>
                </Tab>
                <Tab text="Lab 2">
                    <content>
                          <TableView fx:id="lab2" prefHeight="453.0" prefWidth="397.0" stylesheets="@Stylesheet.css">
                              <columns>
                                  <TableColumn prefWidth="450.0" text="ID" />
                                  <TableColumn fx:id="lab2name" prefWidth="450.0" text="Machine Name" />
                                  <TableColumn fx:id="lab2os" prefWidth="199.0" text="OS" />
                              </columns>
                          </TableView>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
   </center>
   <left>
      <VBox alignment="TOP_RIGHT" prefHeight="1080.0" prefWidth="280.0" styleClass="root2" BorderPane.alignment="CENTER">
         <children>
            <Region prefHeight="100.0" prefWidth="280.0" />
                <Button mnemonicParsing="false" onAction="#home" prefHeight="100.0" prefWidth="240.0" styleClass="button3" text="Home">
               <font>
                  <Font size="33.0" />
               </font>
            </Button>
            <Region prefHeight="50.0" prefWidth="280.0" />
                <Button mnemonicParsing="false" onAction="#switchToMachine" prefHeight="100.0" prefWidth="240.0" styleClass="button3" text="Machines">
               <font>
                  <Font size="33.0" />
               </font>
            </Button>
            <Region prefHeight="50.0" prefWidth="280.0" />
                <Button mnemonicParsing="false" onAction="#switchToSQL" prefHeight="100.0" prefWidth="240.0" styleClass="button3" text="SQL Connect">
               <font>
                  <Font size="33.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </left>
   <right>
      <VBox alignment="TOP_CENTER" prefHeight="1080.0" prefWidth="540.0" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="TOP_RIGHT">
               <children>
                    <Button mnemonicParsing="false" onAction="#logout" prefHeight="73.0" prefWidth="174.0" styleClass="button1" text="Logout">
                     <font>
                        <Font size="30.0" />
                     </font>
                     <HBox.margin>
                        <Insets right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <Region prefHeight="106.0" prefWidth="540.0" />
            <HBox alignment="CENTER" prefHeight="123.0" prefWidth="384.0">
               <children>
                  <TextField prefHeight="75.0" prefWidth="369.0" promptText="Filter">
                     <font>
                        <Font size="28.0" />
                     </font>
                  </TextField>
                  <Region prefHeight="200.0" prefWidth="20.0" />
                  <Button mnemonicParsing="false" prefHeight="75.0" prefWidth="120.0" text="Apply">
                     <styleClass>
                        <String fx:value="button" />
                        <String fx:value="button1" />
                     </styleClass>
                     <font>
                        <Font size="28.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <TextArea prefHeight="480.0" prefWidth="519.0" promptText="Type command to run on selected machines:" style="-fx-background-color: 55;" styleClass="text-area" stylesheets="@Stylesheet.css">
               <font>
                  <Font size="28.0" />
               </font>
            </TextArea>
            <Region prefHeight="30.0" prefWidth="540.0" />
            <Button mnemonicParsing="false" prefHeight="65.0" prefWidth="134.0" styleClass="button1" text="Run">
               <font>
                  <Font size="30.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </right>
</BorderPane>
