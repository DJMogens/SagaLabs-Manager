<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sagalabsmanager.controllers.LabsController">
   <children>
      <HBox layoutX="-1.0" layoutY="-2.0" prefHeight="400.0" prefWidth="600.0">
         <children>
            <VBox prefHeight="400.0" prefWidth="125.0">
               <children>
                  <Region prefHeight="19.0" prefWidth="126.0" />
                  <Button mnemonicParsing="false" onAction="#home" prefHeight="26.0" prefWidth="128.0" text="Home" />
                  <Region prefHeight="20.0" prefWidth="126.0" />
                  <Button mnemonicParsing="false" onAction="#getLabDetails" prefHeight="26.0" prefWidth="126.0" text="Get Lab Details" />
                  <Region prefHeight="18.0" prefWidth="126.0" />
                  <Button mnemonicParsing="false" onAction="#listResourceGroupsWithLabTag" prefHeight="26.0" prefWidth="127.0" text="List Labs (terminal)" />
                  <Region prefHeight="20.0" prefWidth="126.0" />
                  <Button mnemonicParsing="false" onAction="#switchToMachine" prefHeight="26.0" prefWidth="128.0" text="Machines" />
               </children>
            </VBox>
            <Region prefHeight="400.0" prefWidth="74.0" />
            <Label prefHeight="400.0" prefWidth="345.0" text="HOME" textAlignment="CENTER">
               <font>
                  <Font name="System Bold Italic" size="73.0" />
               </font>
            </Label>
            <Button mnemonicParsing="false" onAction="#logout" text="Logout" />
         </children>
      </HBox>
   </children>
</AnchorPane>
